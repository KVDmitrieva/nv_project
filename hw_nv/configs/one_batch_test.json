{
  "name": "one_batch_test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 22050,
    "spectrogram": {
      "type": "MelSpectrogram",
      "args": {
      }
    },
    "log_spec": true
  },
  "augmentations": {
    "wave": [],
    "spectrogram": []
  },
  "arch": {
    "type": "Generator",
    "args": {
      "prolog_params": {
        "in_channels": 1,
        "out_channels": 512,
        "kernel_size": 7
      },
      "upsampler_blocks_params": [
        {
          "upsampler_params": {
            "in_channels": 512,
            "out_channels": 256,
            "kernel_size": 16,
            "stride": 8
          }
        },
        {
          "upsampler_params":  {
            "in_channels": 256,
            "out_channels": 128,
            "kernel_size": 16,
            "stride": 8
          }
        },
        {
          "upsampler_params":  {
            "in_channels": 128,
            "out_channels": 64,
            "kernel_size": 4,
            "stride": 2
          }
        },
        {
          "upsampler_params": {
            "in_channels": 64,
            "out_channels": 32,
            "kernel_size": 4,
            "stride": 2
          }
        }
      ],
      "epilog_params": {
        "in_channels": 32,
        "out_channels": 1,
        "kernel_size": 7
      }
    }
  },
  "arch2": {
    "type": "Discriminator",
    "args": {
      "pooling_params": [1, 2, 4],
      "periods": [2, 3, 5, 7, 11],
      "sd_params": {
        "prolog_params": {
          "in_channels": 1,
          "out_channels": 16,
          "kernel_size": 15,
          "stride": 1
        },
        "downsampler_params": [
          {
          "in_channels": 16,
          "out_channels": 64,
          "kernel_size": 41,
          "stride": 4,
          "groups": 4
        },
          {
          "in_channels": 64,
          "out_channels": 256,
          "kernel_size": 41,
          "stride": 4,
          "groups": 16
        },
          {
          "in_channels": 256,
          "out_channels": 1024,
          "kernel_size": 41,
          "stride": 4,
          "groups": 64
        },
          {
          "in_channels": 1024,
          "out_channels": 1024,
          "kernel_size": 41,
          "stride": 4,
          "groups": 256
        }
        ],
        "post_downsampler_params": {
          "in_channels": 1024,
          "out_channels": 1024,
          "kernel_size": 5,
          "stride": 1
        },
        "epilog_params": {
          "in_channels": 1024,
          "out_channels": 1,
          "kernel_size": 3,
          "stride": 1
        }
      },
      "pd_params": {
        "stem_params": [
          {
            "in_channels": 1,
            "out_channels": 64,
            "kernel_size": [5, 1],
            "stride": [3, 1]
        },
          {
            "in_channels": 64,
            "out_channels": 128,
            "kernel_size": [5, 1],
            "stride": [3, 1]
        },
          {
            "in_channels": 128,
            "out_channels": 256,
            "kernel_size": [5, 1],
            "stride": [3, 1]
        },
          {
            "in_channels": 256,
            "out_channels": 512,
            "kernel_size": [5, 1],
            "stride": [3, 1]
        }
        ],
        "poststem_params": {
          "in_channels": 512,
          "out_channels": 1024,
          "kernel_size": [5, 1]
        },
        "epilog_params": {
          "in_channels": 1024,
          "out_channels": 1,
          "kernel_size": [3, 1]
        }
      }
    }
  },
  "data": {
    "train": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "LJspeechDataset",
          "args": {
            "part": "val",
            "limit": 10
          }
        }
      ]
    },
    "val": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "LJspeechDataset",
          "args": {
            "part": "val",
            "limit": 10
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 1e-3,
      "betas": [0.9, 0.98],
      "eps":1e-9
    }
  },
  "loss": {
    "type": "FastSpeechLoss",
    "args": {}
  },
  "metrics": [],
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 100,
      "epochs": 11,
      "anneal_strategy": "cos",
      "max_lr": 1e-3,
      "pct_start": 0.2
    }
  },
  "trainer": {
    "epochs": 10,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "tts_project",
    "len_epoch": 100,
    "grad_norm_clip": 1
  }
}
